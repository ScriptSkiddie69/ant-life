})
Tab4:AddSlider({
	Name = "Walkspeed",
	Min = 20,
	Max = 500,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(Value5)
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value5
	end    
})
Tab4:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 300,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Real",
